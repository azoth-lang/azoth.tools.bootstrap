published class List_Iterator[T out]
{
    let list: temp const List[T];
    var index: size;

    public init(mut self, .list)
    {
        .index = 0;
    }

    public fn \next(mut self) -> aliasable T?
    {
        if .index >= .list.count => return none;
        let value = .list.get(.index);
        .index += 1;
        return value;
    }
}
