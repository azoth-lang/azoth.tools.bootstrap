published class TestOutput
{
    published fn write(mut self, value: string)
    {
        unsafe
        {
            print_utf8(value.bytes, value.byte_count);
        }
    }

    published fn write_line(mut self, line: string)
    {
        unsafe
        {
            print_utf8(line.bytes, line.byte_count);
            let newline: string = "\n";
            print_utf8(newline.bytes, newline.byte_count);
        }
    }

    published fn write_line(mut self)
    {
        unsafe
        {
            let newline: string = "\n";
            print_utf8(newline.bytes, newline.byte_count);
        }
    }
}
